{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "gambling-theme/button-encoded",
	"version": "0.1.0",
	"title": "Button (Special Blocks)",
	"category": "theme-category",
	"icon": "games",
	"description": "This block add custom buttons",
	"supports": {
		"html": false
	},
	"attributes": {
		"anchor": {
			"type": "string",
			"default": ""
		},
		"preview": {
			"type": "boolean",
			"default": false
		},
		"btnStyle": {
			"type": "string",
			"default": ""
		},
		"url": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "button-encoded",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
